{
	"description": "The CodeIgniter framework",
	"name": "codeigniter/framework",
	"type": "project",
	"homepage": "https://codeigniter.com",
	"license": "MIT",
	"support": {
		"forum": "http://forum.codeigniter.com/",
		"wiki": "https://github.com/bcit-ci/CodeIgniter/wiki",
		"slack": "https://codeigniterchat.slack.com",
		"source": "https://github.com/bcit-ci/CodeIgniter"
	},
	"require": {
      "php": ">=5.6"
	},
	"autoload": {
		"psr-4": {
            "Bbdgnc\\Base\\" : "application/libraries/base",
			"Bbdgnc\\Finder\\" : "application/libraries/finder",
			"Bbdgnc\\Finder\\Enum\\" : "application/libraries/finder/enums",
			"Bbdgnc\\Finder\\Exception\\" : "application/libraries/finder/exceptions",
			"Bbdgnc\\Enum\\" : "application/libraries/enums",
			"Bbdgnc\\Smiles\\" : "application/libraries/smiles",
          "Bbdgnc\\Smiles\\enums\\": "application/libraries/smiles/enums",
          "Bbdgnc\\Smiles\\parsers\\": "application/libraries/smiles/parsers",
			"Bbdgnc\\Exception\\" : "application/libraries/exceptions"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "4.* || 5.*"
	}
}
